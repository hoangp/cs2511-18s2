            <ImageView fx:id="heroImage" disable="true" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../res/human_new.png" />
               </image>
            </ImageView>
            <ImageView fx:id="wallImage" fitHeight="50.0" fitWidth="100.0" layoutX="50.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../res/brick_brown_0.png" />
               </image>
            </ImageView>
            <ImageView fx:id="exitImage" fitHeight="50.0" fitWidth="50.0" layoutY="50.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../res/exit.png" />
               </image>
            </ImageView>
            <ImageView fx:id="treasureImage" fitHeight="50.0" fitWidth="50.0" layoutY="50.0" pickOnBounds="true" preserveRatio="true">
