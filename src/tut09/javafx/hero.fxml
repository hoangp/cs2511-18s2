      <StackPane focusTraversable="true" prefHeight="50.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="backButton" alignment="CENTER" cancelButton="true" contentDisplay="CENTER" focusTraversable="false" mnemonicParsing="false" onAction="#handleBackButton" text="Go Back" />
         </children>
      </StackPane>
   </bottom>
   <top>
      <ToolBar focusTraversable="false" prefHeight="50.0" prefWidth="200.0" BorderPane.alignment="CENTER">
